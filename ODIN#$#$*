	UICorner_2.Parent = BLOOD

	MoneyDupe.Name = "Money Dupe"
	MoneyDupe.Parent = Frame
	MoneyDupe.BackgroundColor3 = Color3.fromRGB(88, 88, 88)
	MoneyDupe.Position = UDim2.new(0.280701756, 0, 0.532726109, 0)
