coroutine.wrap(ZVXJJA_fake_script)()
local function FMJDJFW_fake_script() -- Button_2.LocalScript 
	local script = Instance.new('LocalScript', Button_2)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.Home.Visible = false
		script.Parent.Parent.Scripts.Visible = true
		script.Parent.Parent.Games.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(FMJDJFW_fake_script)()
