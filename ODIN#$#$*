OpenGUI.Text = "OpenGUI"
OpenGUI.TextColor3 = Color3.fromRGB(255, 255, 255)
OpenGUI.TextSize = 14.000
OpenGUI.MouseButton1Down:connect(function()
	--||Este LocalScript va en el boton Open/Close||--

	local button = script.Parent
	local frame = script.Parent.Parent.Frame
	local X = script.Parent.Parent.Frame.TextButton

	button.MouseButton1Down:Connect(function()
		if frame.Visible == false then
			frame.Visible = true
			button.Text = "Close"
		else
			frame.Visible = false
			button.Text = "Open"
		end
	end)

	X.MouseButton1Down:Connect(function()
		frame.Visible = false
		button.Text = "Open"
	end)

	--__________--
	--(By porqyu5)--
end)

