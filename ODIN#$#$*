OpenGUI.Name = "OpenGUI"
OpenGUI.Parent = ScrollingFrame
OpenGUI.BackgroundColor3 = Color3.fromRGB(255, 41, 41)
OpenGUI.Position = UDim2.new(0.0332480818, 0, 0.375077307, 0)
OpenGUI.Size = UDim2.new(0, 356, 0, 23)
