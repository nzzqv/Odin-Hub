				HatChoice.Handle:FindFirstChildOfClass("AlignOrientation").Attachment1 = atho

				athp.Position = Vector3.new(0,-1.75,0)
				atho.Rotation = Vector3.new(0,0,48)
			end
			LWP3.Parent = leftWing
			local RWP1 = WingPiece:Clone();
			if MPASword[1] then
